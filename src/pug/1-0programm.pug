extends _layout

block title
  title Программа

block content
  .pagehead-wrap.text-left(style='background-image: url(img/hbg.png)')
    .inner-content
      .half
        .half-base
          .pagehead-content
            h1.h-title.h1.u-white Программа питания 
              span.u-number 2300–2900
                span.programm-value ккал
                  br
                  | день
          .bread-container
            mixin bread(name)
              a(href="#").icon-link
                i 
                span #{name}
            +bread('Главная')
            +bread('Программы питания')
            +bread('2300-2600 ккал/день')
