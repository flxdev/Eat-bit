      .half-side
        .sticky-wrap.js-sticky
          .white-box
            .h-title.h2 Заказать
            form(action=" ").js-validate
              ul.checkbox-wrap
                mixin Check(name)
                  li.checkbox-item
                    label.checkbox-outer 
                      input.input-real(type="radio",name='group',value = name)
                      span.input-fake #{name}
                +Check('2300-2600 ккал')
                +Check('2600-2900 ккал')
                +Check('2900-3200 ккал')
              .slider-wrap.js-slider-wrap
                .slider-text-wrapper
                  .slider-weeks
                    span.js-slider-week 2 недели
                    span.discount.js-slider-discount -16%
                  .slider-amount
                    span.js-slider-amount 1500 
                    |₽/день
                #rangeinput
                  i.dot.dot25
                  i.dot.dot50
                  i.dot.dot0
                  i.dot.dot100






















  